<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Filter</key>
  <dict>
    <!-- Hook the Infuse app by bundle identifier and executable name -->
    <key>Bundles</key>
    <array>
      <string>com.firecore.infuse</string>
    </array>

    <key>Executables</key>
    <array>
      <string>Infuse</string>
    </array>

    <!-- Mode "Any" is typical; you can change to "Application" if desired -->
    <key>Mode</key>
    <string>Any</string>
  </dict>
</dict>
</plist>
